{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "target": "es6",
    "module": "es6",
    "outDir": "./build",
    "forceConsistentCasingInFileNames": true,
    "declaration": true,
    "noEmit": false,
    "allowImportingTsExtensions": false,
  },
  "include": ["src", "__tests__"],
  "exclude": ["**/__tests__/*"],
}